package PostProcessors;

import Common.ColorOutput;
import Common.FeatureList;
import LightDJGUI.GenericKnob;

public class WhiteBurst extends PostProcessor {

	GenericKnob lengthKnob;
	
	public WhiteBurst(double updatesPerSecond) {
		super(updatesPerSecond);
		

	}

	@Override
	public String getName() {
		return "Burst";
	}

	@Override
	public void init() {
		
		lengthKnob = new GenericKnob(0.5f, 50, "Length");
		requestUserControl(lengthKnob);
		
	}

	@Override
	public ColorOutput visualize(FeatureList featureList) {
		// TODO Auto-generated method stub
		return null;
	}

}
